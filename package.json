{
  "name": "beta",
  "version": "1.11.1",
  "description": "pnut.io client",
  "author": "sunya9 <mugen.xyz@gmail.com> (https://unsweets.net)",
  "private": true,
  "main": "app.js",
  "scripts": {
    "dev": "nuxt",
    "build": "cross-env NODE_ENV=production nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "precommit": "lint-staged",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "eslint --ext .js,.vue --fix .",
    "test": "jest",
    "test:all": "run-p 'test --coverage' lint",
    "test:reporter": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "lint-staged": {
    "*.@(js|vue)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.4",
    "@fortawesome/free-brands-svg-icons": "5.3.1",
    "@fortawesome/free-regular-svg-icons": "5.3.1",
    "@fortawesome/free-solid-svg-icons": "5.3.1",
    "@fortawesome/vue-fontawesome": "0.1.1",
    "@nuxtjs/auth": "4.5.1",
    "@nuxtjs/axios": "5.3.1",
    "@nuxtjs/component-cache": "1.1.2",
    "@nuxtjs/google-analytics": "2.0.2",
    "@nuxtjs/pwa": "2.0.8",
    "@nuxtjs/toast": "3.0.1",
    "bootstrap": "4.1.1",
    "bootstrap.native": "2.0.23",
    "cross-env": "5.1.6",
    "dotenv": "6.0.0",
    "emoji-datasource-twitter": "4.0.4",
    "emoji-mart-vue": "2.6.3",
    "imports-loader": "0.8.0",
    "jquery": "3.3.1",
    "lodash": "4.17.10",
    "moment": "2.22.1",
    "mousetrap": "1.6.1",
    "nuxt": "1.4.2",
    "popper.js": "1.14.3",
    "sass-loader": "7.0.2",
    "string-length": "2.0.0",
    "tether": "1.4.4",
    "twemoji": "11.0.0",
    "unicode-substring": "1.0.0",
    "vue-infinite-scroll": "2.0.2",
    "vue-on-click-outside": "1.0.3",
    "vue-scrollto": "2.11.0",
    "vuedraggable": "2.16.0",
    "zooming": "2.0.0"
  },
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.24",
    "axios-mock-adapter": "1.15.0",
    "codeclimate-test-reporter": "0.5.0",
    "eslint": "5.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-jest": "21.22.0",
    "eslint-plugin-vue": "4.5.0",
    "husky": "0.14.3",
    "imagemin-lint-staged": "0.3.0",
    "jest": "23.5.0",
    "jest-localstorage-mock": "2.2.0",
    "lint-staged": "7.1.2",
    "node-sass": "4.9.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.12.1",
    "vue-jest": "2.6.0"
  },
  "homepage": "https://github.com/sunya9/beta",
  "bugs": "https://github.com/sunya9/beta/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunya9/beta.git"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^[@~]/(.*)$": "<rootDir>/$1",
      "^helper$": "<rootDir>/__tests__/helper",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/fixtures/file.js"
    },
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/__tests__/setup"
    ],
    "testRegex": "(/__tests__/(components|pages)/.*|(\\.|/)(test|spec))\\.jsx?$",
    "transformIgnorePatterns": [
      "node_modules/(?!(nuxt)/)"
    ]
  }
}
